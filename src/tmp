void Client::part_of_each_channels(std::string const &quit_msg)
{

	for (std::map<ChannelName, Channel *const>::const_iterator it = this->_joined_channels_by_name.begin();
	     it != this->_joined_channels_by_name.end();
	     ++it)
	{
		std::vector<std::string> channel_and_msg;
		
		channel_and_msg.push_back(it->first);
		channel_and_msg.push_back(quit_msg);
		part(this, channel_and_msg);
	}
}
